{
    "version": "4.0.5.2-7.3",
    "description": "Memcache module provides handy procedural and object-oriented interface to memcached, highly effective caching daemon, which was especially designed to decrease database load in dynamic web applications",
    "homepage": "https://windows.php.net/",
    "license": {
        "identifier": "PHP-3.01",
        "url": "https://secure.php.net/license/"
    },
    "architecture": {
        "64bit": {
            "url": "https://windows.php.net/downloads/pecl/releases/memcache/4.0.5.2/php_memcache-4.0.5.2-7.3-ts-vc15-x64.zip",
            "hash": "88360db59dd3e7b13803965d7842feb18ac4f61727961c3e83a6a6d956dc6406"
        },
        "32bit": {
            "url": "https://windows.php.net/downloads/pecl/releases/memcache/4.0.5.2/php_memcache-4.0.5.2-7.3-ts-vc15-x86.zip",
            "hash": "265d9469b12e2f7c491ed8bf19b9a596823fbf77f4bd7479e50a2c4bb5b7c15b"
        }
    },
    "depends": [
        "versions/php73"
    ],
    "post_install": [
        "$phpconfd = \"$persist_dir\\..\\php73\\cli\\conf.d\"",
        "$ini = \"extension=$dir\\php_memcache.dll\"",
        "if(!(test-path $phpconfd\\memcache.ini)) {",
        "    Write-Output \"Enabling extension $(Convert-Path $phpconfd)\\memcache.ini\"",
        "    Add-Content -Value $ini -Path \"$phpconfd\\memcache.ini\"",
        "} else {",
        "    Write-Host -f Yellow \"PHP 7.3 was not installed through scoop, you have to activate php_memcache manually! Add the following:`n\"",
        "    Write-Host -f Cyan \"$ini`n`n\"",
        "}"
    ],
    "post_uninstall": [
        "$phpconfd = \"$persist_dir\\..\\php73\\cli\\conf.d\"",
        "Remove-Item \"$phpconfd\\memcache.ini\""
    ]
}
